{
	"expo": {
		"name": "WorkHQ",
		"slug": "workhq",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "automatic",
		"scheme": "workhq",
		"splash": {
			"image": "./assets/splash-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#0B5FFF"
		},
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.workhq.app",
			"config": {
				"googleMapsApiKey": "AIzaSyAPzHKUv5HCr3PbM9v-cV5gggH7VZdXSv0"
			}
		},
		"android": {
			"icon": "./assets/icon.png",
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#0B5FFF"
			},
			"package": "com.workhq.app",
			"edgeToEdgeEnabled": true,
			"predictiveBackGestureEnabled": false,
			"permissions": [
				"ACCESS_FINE_LOCATION",
				"ACCESS_COARSE_LOCATION",
				"android.permission.ACCESS_COARSE_LOCATION",
				"android.permission.ACCESS_FINE_LOCATION"
			],
			"config": {
				"googleMaps": {
					"apiKey": "AIzaSyAPzHKUv5HCr3PbM9v-cV5gggH7VZdXSv0"
				}
			}
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-location",
				{
					"locationAlwaysAndWhenInUsePermission": "Allow WorkHQ to use your location for attendance tracking."
				}
			]
		],
		"extra": {
			"router": {},
			"eas": {
				"projectId": "f5d2d8e4-fe4a-42a6-b0dc-5c72d4bc0a53"
			}
		},
		"owner": "rifu"
	}
}